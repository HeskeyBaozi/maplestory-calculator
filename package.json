{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@ant-design/pro-form": "^1.15.2",
    "@ant-design/pro-layout": "^6.13.0",
    "@umijs/preset-react": "1.x",
    "antd": "^4.11.2",
    "mobx": "^6.1.3",
    "mobx-react": "^7.1.0",
    "mobx-state-tree": "^5.0.0",
    "umi": "^3.3.7"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.3.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.3",
    "yorkie": "^2.0.0"
  }
}
